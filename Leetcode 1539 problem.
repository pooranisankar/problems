#include<iostream>
#include<vector>
using namespace std;
class Solution {
public:
	int findkthpositive(vector<int>& arr, int k) {
		int count=0;
		int num=1;
		int i=0;
		while(true) {
			if(i<arr.size() && arr[i]==num) {
				i++;
			} else {
				count++;
				if(count==k) {
					return num;
				}
			}
			num++;
		}
	}
};
int main() {
	Solution sol;
	vector<int> arr= {2,3,4,5,6};
	int k=4;
	cout<<"kth positive integer is="<<sol.findkthpositive(arr,k)<<endl;
	return 0;
}